    "build-linux": "npm run build && build --linux --x64"
  },
  "devDependencies": {
    "electron": "^8.1.1",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "electron-builder": "^22.4.1"
  }
}
